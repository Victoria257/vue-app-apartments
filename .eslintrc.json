{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/vue3-recommended", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "vue/no-missing-import": "error"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".vue", ".scss"]
      }
    }
  }
}
